{
  "name": "antonysemenov",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=dev node server.js",
    "test": "mocha --compilers js:babel-core/register --require ./tests/test_helper.js './**/*.test.@(js|jsx)'",
    "test:watch": "npm test -- --watch",
    "deploy": "node build.js"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "l10n": "^0.2.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-slick": "^0.13.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.13.0",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.4",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "postcss": "^5.1.2",
    "postcss-loader": "^0.11.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  },
  "keywords": [],
  "author": "Anton Semenov",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "stage-1",
      "react"
    ]
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "parser": "babel-eslint",
    "rules": {
      "quotes": [
        1,
        "single"
      ],
      "strict": [
        2,
        "never"
      ],
      "semi": [
        0,
        "always"
      ],
      "no-unused-vars": [
        1
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2,
      "no-alert": 0
    },
    "extends": "eslint:recommended",
    "plugins": [
      "react"
    ]
  }
}
